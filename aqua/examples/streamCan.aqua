func accumRes(a: string, b: string) -> []?string:
  res_accum: *?string
  str: ?string
  str <<- a
  str2: ?string
  str2 <<- b
  res_accum <<- str
  res_accum <<- str2
  res_accum <<- nil
  <- res_accum