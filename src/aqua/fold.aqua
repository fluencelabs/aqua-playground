import "println.aqua"
import "on.aqua"

-- showcases `for` instruction that compiles to `fold` in AIR
func iterateAndPrint(strings: []string):
    for s <- strings:
        print(s)

func iterateAndPrintParallel(nodes: []string, c: ExternalAddresses -> ()):
    for s <- nodes par:
        on s:
            ads <- Peer.identify
            c(ads)
