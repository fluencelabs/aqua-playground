import "builtin.aqua"

service ParAbb("parabb"):
    call: -> string

-- here we go to another node and not waiting for execution there
-- all `ParAbb.call()` will be executed instantly
func parFunc( node: string, c: Info -> () ):
    y <- ParAbb.call()
    par on node:
        t <- Peer.identify()
        c(t)
    par x <- ParAbb.call()
